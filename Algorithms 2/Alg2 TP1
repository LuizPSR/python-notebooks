  ret = []\n","    while len(ret) < size:\n","        ret.append([random.randint(init, final), random.randint(init, final)])\n","    return ret"],"metadata":{"id":"FANK-4pgigr1","executionInfo":{"status":"ok","timestamp":1667183108726,"user_tz":180,"elapsed":25,"user":{"displayName":"Thiago Henrique","userId":"06802561175237881442"}}},"execution_count":809,"outputs":[]},{"cell_type":"code","source":["pontos1 = np.array(method2(30))\n","pontos2 = np.array(method2(30, init = 100, final = 200))"],"metadata":{"id":"1KJe5G9pi539","executionInfo":{"status":"ok","timestamp":1667183108726,"user_tz":180,"elapsed":24,"user":{"displayName":"Thiago Henrique","userId":"06802561175237881442"}}},"execution_count":810,"outputs":[]},{"cell_type":"code","source":["hull2 =gift_wrapping(pontos1)\n","hull3 =gift_wrapping(pontos2)"],"metadata":{"id":"5YbIW7wwi8FH","executionInfo":{"status":"ok","timestamp":1667183108727,"user_tz":180,"elapsed":24,"user":{"displayName":"Thiago Henrique","userId":"06802561175237881442"}}},"execution_count":811,"outputs":[]},{"cell_type":"code","source":["scatter_plot(pontos1,hull2)\n","scatter_plot(pontos2,hull3)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":0},"id":"FnDEcYDqjOQL","executionInfo":{"status":"ok","timestamp":1667183108727,"user_tz":180,"elapsed":24,"user":{"displayName":"Thiago Henrique","userId":"06802561175237881442"}},"outputId":"946b310c-981e-434c-f795-a6b839a7dac1"},"execution_count":812,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}},{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":["min_dist, min_dist_points = findMinDistance(hull2, hull3)\n","ponto_medio = findMidpoint(min_dist_points)"],"metadata":{"id":"6l_wstEMjbev","executionInfo":{"status":"ok","timestamp":1667183108728